#    Object Properties
#################################################################

###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico#1rBebida
<http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico#1rBebida> rdf:type owl:ObjectProperty ;
                                                                           rdfs:subPropertyOf owl:topObjectProperty ;
                                                                           rdf:type owl:FunctionalProperty ;
                                                                           rdfs:domain :Menu ;
                                                                           rdfs:range :Bebida ;
                                                                           rdfs:comment "Relación para indicar la primera bebida del menú." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico#1rPlato
<http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico#1rPlato> rdf:type owl:ObjectProperty ;
                                                                          rdfs:subPropertyOf owl:topObjectProperty ;
                                                                          rdf:type owl:FunctionalProperty ;
                                                                          rdfs:domain :Menu ;
                                                                          rdfs:range :Plato ;
                                                                          rdfs:comment "Relación para indicar el primer plato del menú." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico#2oBebida
<http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico#2oBebida> rdf:type owl:ObjectProperty ;
                                                                           rdfs:subPropertyOf owl:topObjectProperty ;
                                                                           rdf:type owl:FunctionalProperty ;
                                                                           rdfs:domain :Menu ;
                                                                           rdfs:range :Bebida ;
                                                                           rdfs:comment "Relación para indicar la segunda bebida del menú." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico#2oPlato
<http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico#2oPlato> rdf:type owl:ObjectProperty ;
                                                                          rdfs:subPropertyOf owl:topObjectProperty ;
                                                                          rdf:type owl:FunctionalProperty ;
                                                                          rdfs:domain :Menu ;
                                                                          rdfs:range :Plato ;
                                                                          rdfs:comment "Relación para indicar el segundo plato del menú." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/compuestoPor
:compuestoPor rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Plato ;
              rdfs:range :Ingrediente ;
              rdfs:comment "Relación para indicar que ingredientes componen un plato." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/disponibleEn
:disponibleEn rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Ingrediente ;
              rdfs:range :Temporada ;
              rdfs:comment "Relación para indicar en qué temporada está disponible un ingrediente." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/esCategoria
:esCategoria rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Plato ;
             rdfs:range :Categoria ;
             rdfs:comment "Relación para indicar la categoría de un plato." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/esPreparacion
:esPreparacion rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :Plato ;
               rdfs:range :Preparacion ;
               rdfs:comment "Relación para indicar el tipo de preparación de un plato." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/incompatibleConBebida
:incompatibleConBebida rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdfs:domain :Plato ;
                       rdfs:range :Bebida ;
                       rdfs:comment "Relación para indicar que bebidas son incompatibles con un plato." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/incompatibleConPlato
:incompatibleConPlato rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain :Plato ;
                      rdfs:range :Plato ;
                      rdfs:comment "Relación para indicar que platos son incompatibles con un plato concreto." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/originarioDe
:originarioDe rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :Plato ;
              rdfs:range :Origen ;
              rdfs:comment "Relación para indicar el país de origen de un plato." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/postre
:postre rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdf:type owl:FunctionalProperty ;
        rdfs:domain :Menu ;
        rdfs:range :Plato ;
        rdfs:comment "Relación para indicar el postre de un menú." .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/alcoholica
:alcoholica rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Bebida ;
            rdfs:range xsd:boolean ;
            rdfs:comment "Atributo de tipo Booleano para indicar si una bebida casual es alcohólica o no." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/dificultad
:dificultad rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Plato ;
            rdfs:range xsd:integer ;
            rdfs:comment "Atributo de tipo Integer para indicar la dificultad de realizar un plato. Su rango va de 1 a 10, siendo 1 el valor que indica menor dificultad y 10 el valor que indica la máxima dificultad." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/glutenFree
:glutenFree rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Bebida ,
                        :Ingrediente ;
            rdfs:range xsd:boolean ;
            rdfs:comment "Atributo de tipo Booleano para indicar si es elemento es libre de gluten." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/lactosaFree
:lactosaFree rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Bebida ,
                         :Ingrediente ;
             rdfs:range xsd:boolean ;
             rdfs:comment "Atributo de tipo Booleano para indicar si el elemento es libre de lactosa." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/nombre
:nombre rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdf:type owl:FunctionalProperty ;
        rdfs:domain :Bebida ,
                    :Categoria ,
                    :Ingrediente ,
                    :Menu ,
                    :Origen ,
                    :Plato ,
                    :Preparacion ,
                    :Temporada ;
        rdfs:range xsd:string ;
        rdfs:comment "Atributo de tipo String para indicar el nombre del elemento en cuestión." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/precio
:precio rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdf:type owl:FunctionalProperty ;
        rdfs:domain :Bebida ,
                    :Plato ;
        rdfs:range xsd:float ;
        rdfs:comment "Atributo de tipo Float para indicar el precio del elemento en cuestión." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/tipo
:tipo rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf owl:topDataProperty ;
      rdf:type owl:FunctionalProperty ;
      rdfs:domain :Plato ;
      rdfs:range xsd:integer ;
      rdfs:comment "Atributo de tipo Integer que indica el tipo de plato (Primero, Segundo, Postre). 0 -> Postre // 1 -> Primero // 2 -> Segundo // 3 -> Primero o Segundo" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/Bebida
:Bebida rdf:type owl:Class ;
        rdfs:comment "Clase para representar una bebida." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/Casual
:Casual rdf:type owl:Class ;
        rdfs:subClassOf :Bebida ;
        owl:disjointWith :Vino ;
        rdfs:comment "Clase para representar una bebida casual." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/Categoria
:Categoria rdf:type owl:Class ;
           rdfs:comment "Clase para representar la categoría de un plato." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/Ingrediente
:Ingrediente rdf:type owl:Class ;
             rdfs:comment "Clase para representar un ingrediente de un plato." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/Menu
:Menu rdf:type owl:Class ;
      rdfs:comment "Clase para representar un menú." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/Origen
:Origen rdf:type owl:Class ;
        rdfs:comment "Clase para representar el origen de un plato." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/Plato
:Plato rdf:type owl:Class ;
       rdfs:comment "Clase para representar un plato." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/Preparacion
:Preparacion rdf:type owl:Class ;
             rdfs:comment "Clase para representar el método de preparación de un plato." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/Temporada
:Temporada rdf:type owl:Class ;
           rdfs:comment "Clase para representar la temporada en la que está disponible un ingrediente." .


###  http://www.semanticweb.org/polaracil/ontologies/2025/3/ricorico/Vino
:Vino rdf:type owl:Class ;
      rdfs:subClassOf :Bebida ;
      rdfs:comment "Clase para representar un vino." .


